{% extends 'base.html.twig' %}

{% block title %}Hello RequeteController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">

    {{ form_start(form) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

    <div class="row">
        <div class="col">
            {{ form_row(form.type) }}
        </div>
        <div class="col" id="some-custom-id1">
            {{ form_row(form.url) }}
        </div>
        <h1>Headers</h1><br>
        <div class="col" id="some-custom-id2">
            {{ form_row(form.key) }}
            {{ form_row(form.value) }}
        </div>
    </div>
    {{ form_end(form) }}
</div>
    {% for ligne in result %}

        [id] {{ ligne.id }}
        [prenom] {{ ligne.firstName }}
        [nom] {{ ligne.lastName }} <br>
    {% endfor %}
{% endblock %}
